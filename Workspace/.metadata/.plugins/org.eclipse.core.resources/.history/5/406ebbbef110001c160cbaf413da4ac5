package com.singlylinkedlist;

public class SinglyLinkedList {

	Node head;
	
   class Node{
		int data;
		Node next;
		Node(int d){
			data = d;
			next = null;
		}
		
		public void Display() {
			Node n = head;
			while(n != null) {
				System.out.println("Data: "+n.data+"\n");
				n = n.next();
			}
		}
	}
	public static void main(String[] args) {
		// TODO Auto-generated method stub

	}

}
